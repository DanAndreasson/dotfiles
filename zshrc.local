# Export oh-my-zsh
export ZSH=/Users/dan/.oh-my-zsh

ZSH_THEME="honukai"

CASE_SENSITIVE="false"

# Plugins
plugins=(git zsh-completions colorize github brew osx zsh-syntax-highlighting)

autoload -U compinit && compinit

# Enable vi-mode
bindkey -v

. /usr/local/etc/profile.d/z.sh

source ~/.zshenv
